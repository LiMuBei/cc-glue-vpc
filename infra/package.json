{
    "name": "@glue-vpc-article/infra",
    "version": "1.0.0",
    "main": "main.js",
    "types": "main.ts",
    "license": "MPL-2.0",
    "private": true,
    "scripts": {
        "clean": "echo 'Cleaning infra...' && rm -rf node_modules && rm -rf dist",
        "get": "cdktf get",
        "build": "tsc",
        "synth": "cdktf synth",
        "diff": "cdktf diff glue-vpc-article",
        "compile": "tsc --pretty",
        "watch": "tsc -w",
        "test": "jest",
        "test:watch": "jest --watch",
        "upgrade-cdktf": "yarn add cdktf@latest cdktf-cli@latest",
        "upgrade-cdktf:next": "yarn add cdktf@next cdktf-cli@next"
    },
    "engines": {
        "node": ">=14.0"
    },
    "dependencies": {
        "@cdktf/provider-aws": "16.0.6",
        "@cdktf/provider-random": "8.0.0",
        "cdktf": "0.17.1",
        "constructs": "^10.2.69"
    },
    "devDependencies": {
        "@types/jest": "^29.5.1",
        "@types/node": "^20.1.3",
        "jest": "^29.5.0",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    }
}
